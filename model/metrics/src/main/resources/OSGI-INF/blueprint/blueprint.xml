<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
  <bean id="categoricalMetricContributor" class="org.pentaho.model.metrics.contributor.metricManager.impl.CategoricalMetricContributor"> </bean>
  <bean id="numericMetricContributor" class="org.pentaho.model.metrics.contributor.metricManager.impl.NumericMetricContributor"> </bean>
  <bean id="regexAddressMetricContributor" class="org.pentaho.model.metrics.contributor.metricManager.impl.RegexAddressMetricContributor"> </bean>
  <bean id="stringLengthMetricContributor" class="org.pentaho.model.metrics.contributor.metricManager.impl.StringLengthMetricContributor"> </bean>
  <bean id="wordCountMetricContributor" class="org.pentaho.model.metrics.contributor.metricManager.impl.WordCountMetricContributor"> </bean>
  <bean id="cardinalityMetricContributor" class="org.pentaho.model.metrics.contributor.metricManager.impl.CardinalityMetricContributor"> </bean>

  <service interface="org.pentaho.profiling.api.metrics.MetricManagerContributor" ref="categoricalMetricContributor"/>
  <service interface="org.pentaho.profiling.api.metrics.MetricManagerContributor" ref="numericMetricContributor"/>
  <service interface="org.pentaho.profiling.api.metrics.MetricManagerContributor" ref="regexAddressMetricContributor"/>
  <service interface="org.pentaho.profiling.api.metrics.MetricManagerContributor" ref="stringLengthMetricContributor"/>
  <service interface="org.pentaho.profiling.api.metrics.MetricManagerContributor" ref="wordCountMetricContributor"/>
  <service interface="org.pentaho.profiling.api.metrics.MetricManagerContributor" ref="cardinalityMetricContributor"/>
</blueprint>
