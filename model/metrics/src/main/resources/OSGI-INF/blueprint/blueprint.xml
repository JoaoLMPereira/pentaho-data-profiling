<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
  <bean id="metricContributorBundle" class="com.pentaho.profiling.api.metrics.bundle.MetricContributorBundleImpl">
    <property name="name" value="PentahoDataProfilingMCBundle"/>
    <property name="classes">
      <list>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.CategoricalMetricContributor
        </value>
        <value type="java.lang.Class">com.pentaho.model.metrics.contributor.metricManager.impl.DateMetricContributor
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.NumericMetricContributor
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.RegexAddressMetricContributor
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.StringLengthMetricContributor
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.WordCountMetricContributor
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.PercentileMetricContributor
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.CardinalityMetricContributor
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.percentile.TDigestHolder
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.percentile.PercentileDefinition
        </value>
        <value type="java.lang.Class">
          com.pentaho.model.metrics.contributor.metricManager.impl.cardinality.HyperLogLogPlusHolder
        </value>
      </list>
    </property>
  </bean>

  <service interface="com.pentaho.profiling.api.metrics.bundle.MetricContributorBundle" ref="metricContributorBundle"/>
</blueprint>
